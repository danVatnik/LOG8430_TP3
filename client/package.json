{
  "name": "muziko",
  "version": "1.0.0",
  "description": "Utiliser plusieurs services de diffusion de musique en continu dans une interface commune",
  "main": "index.js",
  "repository": "https://rami-eid@bitbucket.org/danVatnik/log8430.git",
  "contributors": [
    "Rami Eid",
    "Antoine Proulx",
    "Dan Vatnik"
  ],
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.0.0-beta.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "popper.js": "^1.12.6",
    "q": "^1.5.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-sortablejs": "^1.3.5",
    "sortablejs": "^1.6.1",
    "spotify-web-api-js": "^0.22.1"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.18.13",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "esdoc": "^1.0.3",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.1",
    "npm-run-all": "^4.1.1",
    "rollup": "^0.50.0",
    "rollup-plugin-buble": "^0.16.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3"
  },
  "scripts": {
    "build": "rollup -c config/prod.js",
    "build:dev": "rollup -c config/dev.js -w",
    "browse": "browser-sync start --s --index 'index.html' --files 'index.html, styles.css, build/**/*.js' --no-notify --no-open",
    "start": "npm-run-all --parallel build:dev browse",
    "std": "standard --verbose | snazzy",
    "doc": "./node_modules/.bin/esdoc",
    "test": "jest --coverage --collectCoverageFrom \"src/**\""
  },
  "standard": {
    "ignore": [
      "build"
    ]
  }
}
